{
  "dockerImageBuilder": "cimg/python:3.8",
  "inputs": [
    {
      "label": "What is the project name? ",
      "name": "project_name",
      "type": "text"
    },
    {
      "cache": {
        "active": true,
        "newLabel": "Type new path. ",
        "qty": 3
      },
      "label": "Where do you want to save it? ",
      "name": "project_path",
      "type": "text"
    },
    {
      "default": "React Native",
      "items": [
        "React Native",
        "ReactJS",
        "NodeJS"
      ],
      "label": "Which framework do you want to use ? ",
      "name": "framework",
      "type": "text"
    },
    {
      "default": "false",
      "items": [
        "false",
        "true"
      ],
      "label": "Do you want to run and code now ? ",
      "name": "run",
      "type": "bool"
    }
  ]
}
